{
    "name": "horde/horde-cache",
    "description": "Horde Caching API",
    "type": "library",
    "homepage": "https://pear.horde.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        },
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.5.6",
    "time": "2017-03-20",
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3 || ^7",
        "pear-pear.horde.org/Horde_Compress_Fast": "^1@stable",
        "pear-pear.horde.org/Horde_Exception": "^2@stable",
        "pear-pear.horde.org/Horde_Util": "^2@stable",
        "ext-hash": "*"
    },
    "suggest": {
        "pear-pear.horde.org/Horde_Db": "^2.1@stable",
        "pear-pear.horde.org/Horde_HashTable": "^1@stable",
        "pear-pear.horde.org/Horde_Log": "^2@stable",
        "pear-pear.horde.org/Horde_Memcache": "^2@stable",
        "pear-pear.horde.org/Horde_Mongo": "^1@stable",
        "pear-pecl.php.net/APC": "^3.1.1@stable",
        "ext-eaccelerator": "^0.9.5 <=0.9.5",
        "ext-xcache": "*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Cache": "2.*",
        "pear-horde/Horde_Cache": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Cache": "lib/"
        }
    }
}
